{
  "name": "ts-zxcvbn",
  "version": "4.4.2",
  "description": "realistic password strength estimation",
  "author": "Joni Mertoniemi",
  "license": "MIT",
  "main": "lib/main.js",
  "repository": "jonime/ts-zxcvbn",
  "scripts": {
    "build": "npm run build-lib && npm run build-dist",
    "build-lib": "rimraf lib && tsc",
    "build-dist": "browserify --debug --standalone zxcvbn -p tsify --extension='.ts' -t uglifyify src/main.ts | exorcist --base . dist/zxcvbn.js.map >| dist/zxcvbn.js",
    "prepublish": "npm run build",
    "build-lists": "npm run build-frequency-lists && npm run build-keyboard-adjacency-graphs",
    "build-frequency-lists": "python data-scripts/build_frequency_lists.py data src/frequency_lists.ts",
    "build-keyboard-adjacency-graphs": "python data-scripts/build_keyboard_adjacency_graphs.py src/adjacency_graphs.ts"
  },
  "devDependencies": {
    "browserify": "^11.0.1",
    "exorcist": "^0.4.0",
    "rimraf": "^3.0.2",
    "tsify": "^4.0.1",
    "typescript": "^3.8.3",
    "uglifyify": "^3.0.1"
  },
  "keywords": [
    "password",
    "passphrase",
    "security",
    "authentication",
    "strength",
    "meter",
    "quality",
    "estimation",
    "pattern",
    "cracking",
    "scoring",
    "entropy",
    "bruteforce"
  ]
}
